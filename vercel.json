{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/quote",
      "dest": "/server.js"
    },
    {
      "src": "/css/(.*)",
      "dest": "/public/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/public/js/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/public/images/$1"
    },
    {
      "src": "/(index\\.html)?",
      "dest": "/public/index.html"
    },
    {
      "src": "/about\\.html",
      "dest": "/public/about.html"
    },
    {
      "src": "/services\\.html",
      "dest": "/public/services.html"
    },
    {
      "src": "/team\\.html",
      "dest": "/public/team.html"
    },
    {
      "src": "/quote\\.html",
      "dest": "/public/quote.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
